@using Microsoft.AspNetCore.Components.Web
@namespace Nabu.NetSimWeb.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=0.45" />
    <base href="~/" />
    <link rel="stylesheet" href="_content/Nabu.NetSim.UI/css/bootstrap.min.css" />
    <link href="_content/Blazorise/blazorise.css" rel="stylesheet" />
    <link href="_content/Blazorise.Bootstrap5/blazorise.bootstrap5.css" rel="stylesheet" />
    <link href="_content/Nabu.NetSim.UI/css/fa.css" rel="stylesheet" />
    <link href="_content/Nabu.NetSim.UI/css/site.css" rel="stylesheet" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    @RenderBody()

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <script src="_framework/blazor.server.js"></script>
    <script src="_content/Nabu.NetSim.UI/js/bootstrap.bundle.js"></script>
    <script src="_content/Nabu.NetSim.UI/js/tooltip.enable.js"></script>
</body>
</html>
